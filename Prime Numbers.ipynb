    "def is_prime(n):\n",
    "    if n == 1:\n",
    "        return False\n",
    "\n",
    "    for i in range(2,n):\n",
    "        if n % i == 0:\n",
    "            return False\n",
    "    return True\n",
    "\n",
    "prime_number = []\n",
    "for n in range(1,100):\n",
    "    if is_prime(n) == True:\n",
    "        prime_number.append(n)\n",
    "\n",
    "print(prime_number)"
   ]
