   "source": [
    "\n",
    "n = int(input(\"Enter a positive int number to check if it is a Prime number.\"))\n",
    "counter = 0 \n",
    "for i in range(1, n+1):\n",
    "    if n % i == 0:\n",
    "        counter += 1\n",
    "\n",
    "if (n == 0) or (n == 1) or (counter >=3):\n",
    "    print(n, \" is not a prime number\")\n",
    "\n",
    "else:\n",
    "    print(n, \" is a prime number\")\n"
   ]
  }
